//判断一个数是奇数还是偶数
int main()
{
	int a = 0;
	int b = 0;
	scanf("%d", &a);
	b = a % 2;
	if (1 == b)
		printf("%d为奇数",a);
	else
		printf("%d为偶数",a);
}


//输出1~100之间的奇数
int main()
{
	int a = 1;
	while(a<=100)
	{
		if (1 == a % 2)
				printf("%d\n", a);
    a++;
	}
	return 0;
}


