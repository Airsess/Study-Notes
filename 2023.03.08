int main()
{
	int age = 0;
	scanf("%d", &age);
	if (age < 18)
		printf("未成年\n");
	else if (age < 28)		//else if(age>=18 && age<28)
		printf("青年\n");
	else if (age < 50)		//else if(age<=28 && age<50)
		printf("中年\n");
	else
		printf("老年\n");	
	return 0;
}


